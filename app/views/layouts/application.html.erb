<!DOCTYPE html>
<html>
<head>
  <title>Whiteboard</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/errors' %>

<%= render 'layouts/sidebar' if user_signed_in? %>

<main class="content">
  <%= yield %>
</main>

<%= render 'layouts/footer' if user_signed_in? %>

</body>
</html>
